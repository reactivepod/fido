{
  "name": "@reactive/fido",
  "version": "1.2.0",
  "description": "Search iTunes for reviews for one or more podcasts",
  "main": "lib/index.js",
  "scripts": {
    "tap": "tap test/*.js",
    "test": "npm-run-all transpile tap",
    "lint": "eslint src",
    "run-cli": "node lib/bin/cli.js",
    "fido": "npm-run-all transpile run-cli",
    "babel": "babel src --out-dir lib",
    "transpile": "npm-run-all clean babel",
    "clean": "trash lib",
    "watch": "chokidar 'src/**/*.js' -c 'npm run babel'",
    "prepublish": "npm run transpile"
  },
  "author": "Henning Glatter-Gotz",
  "repository": {
    "type": "git",
    "url": "git@github.com:reactivepod/fido.git"
  },
  "preferGlobal": true,
  "bin": {
    "fido": "./lib/bin/cli.js"
  },
  "license": "MIT",
  "dependencies": {
    "async": "^1.4.2",
    "chalk": "^1.1.1",
    "chrono-node": "^1.0.8",
    "dateformat": "^1.0.11",
    "lodash.isarray": "^3.0.4",
    "request": "^2.61.0",
    "xml2js": "^0.4.15",
    "yam": "0.0.18",
    "yargs": "^3.25.0"
  },
  "devDependencies": {
    "babel-cli": "^6.0.15",
    "babel-eslint": "^4.1.1",
    "babel-preset-es2015": "^6.0.12",
    "chokidar-cli": "^1.1.1",
    "eslint": "^1.8.0",
    "eslint-config-airbnb": "0.1.0",
    "npm-run-all": "^1.2.12",
    "tap": "^2.2.0",
    "trash-cli": "^1.1.0",
    "user-home": "^2.0.0"
  },
  "engines": {
    "node": ">=4.1.2"
  }
}
